{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {"type": "string"},
    "name": {"type": "string"},
    "prereqs": {"type": "array", "items": {"type": "string"}},
    "weights": {
      "type": "object",
      "patternProperties": {
        ".*": {"type": "number"}
      },
      "additionalProperties": false
    }
  },
  "required": ["id", "name"],
  "additionalProperties": false
}
